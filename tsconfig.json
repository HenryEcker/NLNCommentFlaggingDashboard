{
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "isolatedModules": true,
    "strictBindCallApply": false,
    "removeComments": true,
    "module": "ES6",
    "target": "ES2021"
  }
}